{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "no-unused-variable": true,

    "no-submodule-imports": false,
    "object-literal-sort-keys": false,
    "jsx-boolean-value": false,
    "jsx-no-multiline-js": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
